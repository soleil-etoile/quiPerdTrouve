function matchInAlgolia(champ){var matchFull=false,matchPartialFullyHighlighted=false,matchPartial=false;if(champ){$.each(champ,function(i,type){if(type.matchLevel==='full'){matchFull=matchFull||type.value;}
if(type.matchLevel==='partial'){if(type.fullyHighlighted){matchPartialFullyHighlighted=matchPartialFullyHighlighted||type.value;}else{matchPartial=matchPartial ||type.value;}}});}
return matchFull||matchPartialFullyHighlighted||matchPartial;}
