;(function(cn, ys){
    var $ = ys.$;

    ;(function(cn){

        var tokenMappy = 'pagesjaunes';

        /* ----------------------------------------------------------- */
        /* Retourne les options d'un élément pjcarto avec les valeurs par défaut.
         **/
        function _getCompParams($e,d) {
            return  $.extend(true, d ||
            { //  Options par défault
                'lrCarte': false,
                'mobile': false,
                'repertoireImagesLeaflet':  '/vendor/mappy/images/',
                'pathAssets': {
                    'pictos': {
                        'numerote':         'marker_hole.png',
                        'nonNumerote':      'marker_puce.png',
                        'geoloc':           'picto-geoloc.png'
                    }
                },
                'paramMappy': {
                    'position': {
                        'logo': 'bottomleft',
                        'attribution': 'bottomright'
                    }
                },
                'classes': {
                    'popup': {
                        'regular':  'pj-leaflet-popup',
                        'nonNum':   'pj-popup-non-numerote',
                        'cible':    'pj-popup-cible'
                    },
                    'biactif':      'actif',
                    'texte_marker': 'pj-texte-marker',
                    'hide':         'hidden'
                },
                'zoom': 16,
                'centre': [49.721842,0.33175],
                'templates' : {
                    'popups': {
                        'marker':       '#mst_popup_marker',
                        'cible':        '#mst_popup_cible'
                    }
                },
                'btns': {
                },
                'listePastilles' : []

            }, $e.data(cn)||{} );
        }

        /* ----------------------------------------------------------- */
        /* Charge pjcarto.
         **/
        function _load($e) {
            var d = _getCompParams($e);
            if(d.centre === null) return;

            // Création du NumberedDivIcon
            L.NumberedDivIcon = L.Icon.extend({
                options: {
                    iconUrl: d.repertoireImagesLeaflet + d.pathAssets.pictos.numerote,
                    number: '',
                    shadowUrl: null,
                    iconSize: new L.Point(25, 41),
                    iconAnchor: new L.Point(13, 41),
                    popupAnchor: new L.Point(0, -33)
                },

                createIcon: function() {
                    var div = document.createElement('div');
                    var img = this._createImg(this.options['iconUrl']);
                    var numdiv = document.createElement('div');
                    numdiv.setAttribute ( "class", "number" );
                    numdiv.innerHTML = this.options['number'] || '';
                    div.appendChild ( img );
                    div.appendChild ( numdiv );
                    this._setIconStyles(div, 'icon');
                    return div;
                },
                createShadow: function() { return null; }
            });

            // On doit préciser la location du répetoire des images
            L.Icon.Default.imagePath = d.repertoireImagesLeaflet;

            // On active le https si besoin
            if('https:' == window.location.protocol) {
                L.Mappy.enableHttps();
            }

            // --------------------------------------------------------
            // Création de la map a proprement parler
            L.Mappy.setImgPath(d.repertoireLogoMappy);
            ys.head.pjcarte = new L.Mappy.Map($e.attr('id'), {
                'clientId': tokenMappy,
                'center': d.centre,
                'zoom': d.zoom, //  Niveau de zoom
                'layersControl': false,
                'zoomControl': false,
                'logoControl': {
                    'position': d.paramMappy.position.logo,
                    'dir': d.repertoireImagesLeaflet // Chemin vers les logos mappy
                },
                'attributionControl': {
                    'position': d.paramMappy.position.attribution
                },
            });

            // --------------------------------------------------------
            // Gestion des Markers / Pin's
            if(d.listePastilles.length) {
                var mLayer = L.layerGroup().addTo(ys.head.pjcarte), // Création d'un layer contenant les marqueurs à afficher
                    mCoords = [], // Stocke les coordonnées pour centrer par rapport à l'ensemble
                    markersListe = {}, // Tableau de marqueur, utilisé pour faire la correspondance BI LR Carte <=> marqueurs carte
                    _markerListe = []
                ;

                // parcours des marqueurs
                $.each(d.listePastilles, function(i, pastille) {
                    var zi = 1000 - pastille.numeroPastille || 1, // Calcul du zindex si c'est un marque numéroté
                        marker_opts = {};

                    marker_opts = pastille.nonnumerote ? {
                            "pastille": 1, // On stocke le numéro de pastille afin de pouvoir les réorganiser (zindex) plus tard
                            "nonnumerote": 1, // On précise que c'est une puce
                            "icon": new L.Icon({ // Nouvel Icon Leaflet
                                iconUrl: d.repertoireImagesLeaflet + d.pathAssets.pictos.nonNumerote
                            }),
                            "zIndexOffset": zi // Position ( 1000 - 1)
                        }
                    :
                        {
                            "pastille": i,
                            "icon": new L.NumberedDivIcon({ // Nouvel Icon numéroté
                                number:  pastille.numeroPastille,
                                iconUrl: d.repertoireImagesLeaflet + d.pathAssets.pictos.numerote
                            }),
                            "zIndexOffset": zi
                        }

                    pastille.pjtoggleclass &&
                        $.extend(marker_opts, {'pjtoggleclass': pastille.pjtoggleclass})

                    // Création conrête du marqueur, qui est tout de suite ajouté à la map
                    var marker = new L.marker(
                            pastille.coordonnees.coordinates,
                            marker_opts
                        )
                        .addTo(mLayer);

                    // On stocke les coordonnées pour pouvoir centrer sur le milieu de ces marqueurs
                    !pastille.hors_zone &&
                        mCoords.push(pastille.coordonnees.coordinates);

                    _markerListe[i] = marker;
                }); // EO $.each(d.listePastilles, function(i, pastille) {


                /*  ----------------------------------------------------------- */
                /*
                * Contour commune jira #FRONPJFD-1183
                */
                if(window.data_communes != undefined && window.data_communes != null) {

                    data_communes.forEach(function(data_commune) {
                        var polygonPoints = [];
                        ((""+data_commune.geometry.type).toLowerCase() == "polygon" ? [data_commune.geometry.coordinates] : data_commune.geometry.coordinates).forEach(function(poly){
                            poly.forEach(function(lls){
                                lls.forEach(function(ll){
                                    polygonPoints.push(new L.LatLng(ll[1], ll[0]));
                                });
                                ys.head.pjcarte.addLayer(new L.Polygon(polygonPoints));
                                polygonPoints = [];
                            });
                        });

                        L.marker([data_commune.properties.x, data_commune.properties.y], {icon: L.divIcon({
                            'html': data_commune.properties.name,
                            'iconSize': null,
                            'className': 'leaflet-nom-ville'
                        }) }).addTo(ys.head.pjcarte);
                        ys.head.pjcarte.addLayer(new L.Polygon(polygonPoints));
                    });
                }

                // On centre sur l'ensemble de marqueurs dont on a stocké les coords */
                mCoords.length &&
                    ys.head.pjcarte.fitBounds(mCoords, {'reset': true, 'padding': [50, 50]}) /*  Padding pour éviter la supperposition bouttons / markers */

                ys.head.pjcarte.dragging.disable();
                ys.head.pjcarte.touchZoom.disable();
                ys.head.pjcarte.doubleClickZoom.disable();
                ys.head.pjcarte.scrollWheelZoom.disable();
                ys.head.pjcarte.boxZoom.disable();
                ys.head.pjcarte.keyboard.disable();
                if (ys.head.pjcarte.tap) ys.head.pjcarte.tap.disable();
            } //  EO if(d.listePastilles != []) {

            ys.head.doAction('ys.pjcarte.endinit');
        }

        /* ----------------------------------------------------------- */
        /* Arrête la propagation devenement à travers une popup leaflet
         **/
        function _stopEvent(e) {
            if(!e) var e = window.event;
            // this will kill the bubbling process.
            e.cancelBubble = true; // e.cancelBubble is supported by IE
            e.returnValue = false;

            // e.stopPropagation works only in Firefox.
            if ( e.stopPropagation ) e.stopPropagation();
            if ( e.preventDefault ) e.preventDefault();

            return false;
        }

        ys.head.onScan('[data-'+cn+']', function(){
            _load($(this));
        });

    })(cn);

})('pjcartecontour', window.ys)
/**
 * PagesJaunes Javascript ys.foot.pj.lr 3.0.27
