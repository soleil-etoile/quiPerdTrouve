previous=now;result=func.apply(context,args);if(!timeout)context=args=null;}else if(!timeout){timeout=setTimeout(later,remaining);}
return result;};throttled.cancel=function(){clearTimeout(timeout);previous=0;timeout=context=args=null;};return throttled;};$(function(){var
