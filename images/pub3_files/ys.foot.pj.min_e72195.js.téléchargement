if(this._.options.shortKeys.hasOwnProperty(p))
this._.options.verboseKeys[this._.options.shortKeys[p]]=p;}},getOptions:function(){return this._.options;},disable:function(){this.clear();this._.options.user_off=1;localStorage.setItem(this._.options.item+'_off',1);this._.ko=true;},enable:function(){this._.options.user_off=0;localStorage.removeItem(this._.options.item+'_off');this._.ko=!this._.options.off;},getHisto:function(){return this._.histo||this._.emptyHisto;},load:function(){if(this._.ko)return;try{var histo_temp=localStorage.getItem(this._.options.item),xss_reg=new RegExp('<script[^>]*>(.*?)<\/script[^>]*>|<javascript[^>]*>(.*?)<\/javascript[^>]*>');if(xss_reg.test(histo_temp)){this.remove(this._.options.item)}else{this._.histo=$.parseJSON(histo_temp);}}catch(e){}
this._.histo=this._.histo||this._.emptyHisto;this._.histo.t=this.changeKeys(this._.histo.t,this._.options.verboseKeys);this.purgeTTL();},save:function(){if(this._.ko)return;var l=this._.emptyHisto;l.t=this.changeKeys(this._.histo.t,this._.options.shortKeys,true);try{localStorage.setItem(this._.options.item,JSON.stringify(l))}catch(e){}},clear:function(){if(this._.ko)return;localStorage.removeItem(this._.options.item);this._.histo=this._.emptyHisto;},purgeTTL:function(){if(this._.ko)return;var s=false,dl=new Date();dl.setDate(dl.getDate()-this._.options.ttl);dl.setHours(0,0,0,0);dl=dl.getTime();for(var i=this._.histo.t.length-1;i>=0;i--){if(this._.histo.t[i][this._.options.dKey]>=dl)break;this._.histo.t.splice(i,1);s=true;}
this.purgeMax(s);},purgeMax:function(doSave){if(this._.ko)return;var s=doSave;while(this._.histo.t.length>this._.options.max_mem){this._.histo.t.pop();s=true;}
s&&this.save();},add:function(h){if(this._.ko)return;h[this._.options.dKey]=ys.head.now();if($.isFunction(this._.options.isSameHit)){for(var i=0,l=this._.histo.t.length;i<l;i++){var cur=this._.histo.t[i];if(this._.options.isSameHit(cur,h)){this._.histo.t.splice(i,1);break;}}}
