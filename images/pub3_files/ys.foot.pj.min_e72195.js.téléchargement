if(opt.etatFantomas){opt.data=opt.data?opt.data:{};opt.data.etatFantomas=false;opt.data.etatFantomas=!!(JSON.parse(window.sessionStorage.getItem('pjhistofantomas'))||{})[opt.etatFantomas];}
if(opt.cadre){opt.data=opt.data?opt.data:{};var _bounds=ys.head.pjcarte.getBounds();opt.data.toprightx=_bounds._northEast.lng,opt.data.toprighty=_bounds._northEast.lat,opt.data.bottomleftx=_bounds._southWest.lng,opt.data.bottomlefty=_bounds._southWest.lat}
